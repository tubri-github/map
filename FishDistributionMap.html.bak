			    // Range
				var dataTime = d3.range(0, 60).map(function(d) {
				return new Date(1950 + d, 10, 3);
			  });
				var minYear = d3.min(dataTime).getFullYear();
				var maxYear = d3.max(dataTime).getFullYear();
				var numYears = maxYear - minYear;
			  var sliderRange = d3
				.sliderBottom()
				.min(d3.min(dataTime))
				.max(d3.max(dataTime))
				//.step(1000 * 60 * 60 * 24 * 365 * 5)
				.width(300)
				.ticks(numYears/5)
				.tickFormat(d3.timeFormat('%Y'))
				//.tickValues(dataTime)
				.default([new Date(1970, 10, 3), new Date(1999, 10, 3)])
				.fill('#2196f3')
				.on('onchange', val => {
				  d3.select('p#value-range').text(d3.timeFormat('%Y')(val));
				});

			  var gRange = d3
				.select('div#slider-range')
				.append('svg')
				.attr('width', 500)
				.attr('height', 100)
				.attr('style', ' padding-left: 70px')
				.append('g')
				.attr('transform', 'translate(30,30)');

			  gRange.call(sliderRange);

			  d3.select('p#value-range').text(
				sliderRange
				  .value()

			  );





